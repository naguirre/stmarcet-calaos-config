        <calaos:condition type="standard">
            <calaos:input id="input_3" oper="==" val="true"/>
        </calaos:condition>
        <calaos:action type="standard">
            <calaos:output id="output_0" val="true"/>
        </calaos:action>
        <calaos:action type="standard">
            <calaos:output id="intern_0" val="" val_var="output_0"/>
