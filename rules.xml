        <calaos:action type="standard">
            <calaos:output id="output_0" val="false"/>
        </calaos:action>
        <calaos:action type="standard">
            <calaos:output id="intern_0" val="" val_var="output_0"/>
        </calaos:action>
    </calaos:rule>
</calaos:rules>
